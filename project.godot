; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Elementalist"
config/version="0.5.1.3"
run/main_scene="res://Scenes/MainMenu.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
run/max_fps=120
boot_splash/show_image=false
config/icon="res://Icon/icon.png"

[autoload]

CustomSignals="*res://Scripts/Global/CustomSignals.cs"
GameState="*res://Scripts/Global/GameState.cs"
Transitions="*res://Scenes/UI/Transitions.tscn"
SettingsManager="*res://Scripts/Global/SettingsManager.cs"
AudioManager="*res://Scenes/AudioManager.tscn"

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/size/mode=4
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="Elementalist Godot"

[editor]

naming/scene_name_casing=1
export/convert_text_resources_to_binary=false

